{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build/",
    "rootDir": "./src/",
    "strict": false,
    "esModuleInterop": true,
    "baseUrl": "./src/",
    "paths": {
      "common/*": ["../../common/src/*"],
      "server/*": ["../../server/src/*"]
    }
  },
  "references": [
    {
      "path": "../common/"
    },
    {
      "path": "../server/"
    }
  ]
}
