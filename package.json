{
  "name": "gradtrak",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:client && cp -R server build && cp -R client/dist/gradtrak build/dist",
    "build-dev": "yarn clean && yarn build-dev:client && cp -R server build && cp -R client/dist/gradtrak build/dist",
    "clean": "rm -rf build",
    "start": "node build/bin/www",
    "watch": "concurrently -n client,server --handle-input \"yarn start:client\" \"yarn watch:server\"",
    "lint": "yarn lint:client && yarn lint:server",
    "lint-fix": "yarn lint-fix:client && yarn lint-fix:server",
    "pretty": "yarn pretty:client && yarn pretty:server",
    "pretty-fix": "yarn pretty-fix:client && yarn pretty-fix:server",
    "start:client": "cd client && ng serve",
    "build:client": "cd client && ng build --prod",
    "build-dev:client": "cd client && ng build --aot",
    "test:client": "cd client && ng test",
    "e2e:client": "cd client && ng e2e",
    "lint:client": "cd client && eslint \"src/app/**/*.ts\"",
    "lint-fix:client": "cd client && eslint --fix \"src/app/**/*.ts\"",
    "pretty:client": "prettier --check \"client/src/app/**/*.{html,scss,ts}\"",
    "pretty-fix:client": "prettier --check --write \"client/src/app/**/*.{html,scss,ts}\"",
    "start:server": "cd server && node ./bin/www",
    "watch:server": "cd server && nodemon ./bin/www",
    "lint:server": "cd server && eslint \"**/*.js\"",
    "lint-fix:server": "cd server && eslint --fix \"**/*.js\"",
    "pretty:server": "prettier --check \"server/**/*.js\"",
    "pretty-fix:server": "prettier --check --write \"server/**/*.js\"",
    "heroku-postbuild": "true"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "argon2": "^0.26.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-redis-cache": "^1.1.3",
    "express-session": "^1.17.0",
    "material-design-icons": "^3.0.1",
    "mongoose": "^5.8.7",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "codelyzer": "^5.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "typescript": "~3.5.3"
  }
}
