{
  "name": "gradtrak",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:server && cp -R server/build build && yarn build:client && cp -R client-react/dist build/dist",
    "build-dev": "yarn clean && yarn build:server && cp -R server/build build && yarn build-dev:client && cp -R client-react/dist build/dist",
    "clean": "rm -rf build common/build common/tsconfig.tsbuildinfo server/build server/tsconfig.tsbuildinfo client-react/build client-react/dist client-react/tsconfig.tsbuildinfo",
    "start": "node build/bin/www",
    "watch": "concurrently -n client,server --handle-input \"yarn start:client\" \"yarn watch:server\"",
    "lint": "yarn lint:client && yarn lint:server",
    "lint-fix": "yarn lint-fix:client && yarn lint-fix:server",
    "pretty": "yarn pretty:client && yarn pretty:server",
    "pretty-fix": "yarn pretty-fix:client && yarn pretty-fix:server",
    "start:client": "cd client-react && webpack serve",
    "build:client": "cd client-react && webpack bundle",
    "build-dev:client": "echo \"Not yet implemented\" && false",
    "lint:client": "cd client-react && eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix:client": "cd client-react && eslint --fix \"src/**/*.{ts,tsx}\"",
    "pretty:client": "prettier --check \"client/src/**/*.{html,scss,ts,tsx}\"",
    "pretty-fix:client": "prettier --check --write \"client/src/**/*.{html,scss,ts,tsx}\"",
    "build:server": "cd server && tsc --build",
    "start:server": "cd server && node ./build/bin/www.js",
    "watch:server": "cd server && nodemon --ext ts --exec \"yarn build:server && yarn start:server\"",
    "lint:server": "cd server && eslint \"src/**/*.ts\"",
    "lint-fix:server": "cd server && eslint --fix \"src/**/*.ts\"",
    "pretty:server": "prettier --check \"server/src/**/*.ts\"",
    "pretty-fix:server": "prettier --check --write \"server/src/**/*.ts\"",
    "heroku-postbuild": "true"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "express-redis-cache": "^1.1.3",
    "express-session": "^1.17.0",
    "material-design-icons": "^3.0.1",
    "memoizee": "^0.4.15",
    "mongoose": "^5.11.11",
    "morgan": "~1.10.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-redis-cache": "^1.1.0",
    "@types/express-session": "^1.17.3",
    "@types/memoizee": "^0.4.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.20",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^4.5.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.3",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "12"
  }
}
