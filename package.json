{
  "name": "gradtrak",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:server && yarn build:client",
    "build-dev": "yarn clean && yarn build:server && yarn build-dev:client",
    "clean": "rm -rf build",
    "start": "node ./build/server/bin/www",
    "watch": "concurrently -n client,server --handle-input \"yarn start:client\" \"yarn watch:server\"",
    "lint": "eslint --cache \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "pretty": "prettier --check \"src/**/*.{js,jsx,ts,tsx,html,css}\"",
    "pretty-fix": "prettier --check --write \"src/**/*.{js,jsx,ts,tsx,html,css}\"",
    "start:client": "cd src/client && webpack serve --config webpack.development.js",
    "build:client": "cd src/client && webpack bundle",
    "build-dev:client": "cd src/client && webpack bundle --config webpack.development.js",
    "build:server": "tsc --build src/server/tsconfig.json",
    "watch:server": "nodemon --watch src/server --ext ts --exec \"yarn build:server && yarn start\"",
    "heroku-postbuild": "true"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "express-redis-cache": "^1.1.3",
    "express-session": "^1.17.0",
    "material-design-icons": "^3.0.1",
    "memoizee": "^0.4.15",
    "mongoose": "^5.11.13",
    "morgan": "~1.10.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@svgr/webpack": "^5.5.0",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-redis-cache": "^1.1.0",
    "@types/express-session": "^1.17.3",
    "@types/memoizee": "^0.4.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap-typeahead": "^5.1.1",
    "@types/react-dom": "^17.0.0",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^4.5.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-bootstrap-typeahead": "^5.1.4",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "12"
  }
}
