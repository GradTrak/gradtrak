{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./build/",
    "rootDir": "./src/",
    "strict": false,
    "esModuleInterop": true,
    "baseUrl": "./src/",
    "paths": {
      "common/*": ["../../common/src/*"]
    }
  },
  "references": [
    {
      "path": "../common/"
    }
  ]
}
